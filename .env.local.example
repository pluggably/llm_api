# Local development example
# Use either Direct URI (if local IPv6 works) or Session pooler URI.

LLM_API_API_KEY=dev-local-key
LLM_API_MODEL_PATH=./models
LLM_API_DATABASE_URL=postgresql+psycopg://postgres:<PASSWORD>@aws-0-<REGION>.pooler.supabase.com:5432/postgres?sslmode=require
LLM_API_DATABASE_SCHEMA=llm_api
LLM_API_PERSIST_STATE=false
LLM_API_LOG_LEVEL=INFO
LLM_API_VERBOSE_LOGS=false
LLM_API_INVITE_REQUIRED=true

# Keep stable if you want persisted encrypted provider keys across restarts.
LLM_API_ENCRYPTION_KEY=dev-change-me

# Optional provider credentials
LLM_API_OPENAI_API_KEY=
LLM_API_ANTHROPIC_API_KEY=
LLM_API_GOOGLE_API_KEY=
LLM_API_AZURE_OPENAI_API_KEY=
LLM_API_AZURE_OPENAI_ENDPOINT=
LLM_API_AZURE_OPENAI_API_VERSION=2024-02-15-preview
LLM_API_XAI_API_KEY=
LLM_API_XAI_BASE_URL=https://api.x.ai/v1
