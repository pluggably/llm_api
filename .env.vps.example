# VPS production example
# Prefer Direct Supabase URI for persistent VPS runtimes.

LLM_API_API_KEY=replace-with-strong-random-key
LLM_API_MODEL_PATH=./models
LLM_API_DATABASE_URL=postgresql+psycopg://postgres:<PASSWORD>@db.<PROJECT>.supabase.co:5432/postgres?sslmode=require
LLM_API_DATABASE_SCHEMA=llm_api
LLM_API_PERSIST_STATE=true
LLM_API_LOG_LEVEL=INFO
LLM_API_VERBOSE_LOGS=false
LLM_API_INVITE_REQUIRED=true

# Required for encrypted user provider keys
LLM_API_ENCRYPTION_KEY=replace-with-strong-stable-secret

# Optional provider credentials (can also be user-scoped in app)
LLM_API_OPENAI_API_KEY=
LLM_API_ANTHROPIC_API_KEY=
LLM_API_GOOGLE_API_KEY=
LLM_API_AZURE_OPENAI_API_KEY=
LLM_API_AZURE_OPENAI_ENDPOINT=
LLM_API_AZURE_OPENAI_API_VERSION=2024-02-15-preview
LLM_API_XAI_API_KEY=
LLM_API_XAI_BASE_URL=https://api.x.ai/v1
LLM_API_HF_TOKEN=
